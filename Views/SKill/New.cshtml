
@{
    ViewBag.Title = "New";
}

<h2>Create a new skill</h2>
<form action="/SKill/Create" method="post">
    <div class="form-group row">
        <label for="name" class="col-sm-1">Name:</label>
        <div class="col-sm-10">
            <input type="text" id="name" name="SkillName" class="form-control"/>
        </div>
        
    </div>
    <div>
        <input type="submit" class="btn btn-primary" value="Add skill" />
    </div>
</form>

