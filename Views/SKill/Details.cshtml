@model TaskTrackingApp.Models.ViewModels.DetailsSkill
@{
    ViewBag.Title = "Details";
}
<a href="/Skill/DeleteConfirm/@Model.selectedSkillDto.SkillID">Confirm a delete</a>
<a href="/Skill/Edit/@Model.selectedSkillDto.SkillID">Update</a>
<h2>Details</h2>
<p>Skill: @Model.selectedSkillDto.SkillName</p>
<p>possessors:</p>
@foreach(var developer in Model.relatedDeveloperDtos)
            {
                <a href="/Developer/Details/@developer.DevID">
                    @developer.DeveloperFirstName @developer.DeveloperLastName
                </a>
            }

